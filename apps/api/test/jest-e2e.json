{
  "moduleFileExtensions": [
    "js",
    "json",
    "ts"
  ],
  "rootDir": ".",
  "testEnvironment": "node",
  "testRegex": ".e2e-spec.ts$",
  "transform": {
    "^.+\\.(t|j)s$": "ts-jest"
  },
  "moduleNameMapper": {
    "@api/(.*)": "<rootDir>/../src/$1",
    "@api": "<rootDir>/../src",
    "@libs/core/(.*)": "<rootDir>/../../../libs/core/src/$1",
    "@libs/core": "<rootDir>/../../../libs/core/src",
    "@libs/infrastructures/(.*)": "<rootDir>/../../../libs/infrastructures/src/$1",
    "@libs/infrastructures": "<rootDir>/../../../libs/infrastructures/src"
  }
}
